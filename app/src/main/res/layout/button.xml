<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"  android:layout_height="10dp" android:layout_width="10dp">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#ff4b9b34" />
            <stroke
                android:width="2dp"
                android:color="#ff914b16" />
            <corners
                android:radius="6dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>

    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ff795b9b"
                android:endColor="#ff795b9b"
                android:angle="270" />
            <stroke
                android:width="4dp"
                android:color="#ff515191" />
            <corners
                android:radius="7dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>