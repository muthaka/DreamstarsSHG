<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dreamstars.muthaka.com.dreamstarsshg" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- View Achievement Activity -->
        <activity
            android:name=".Achievement"
            android:label="View Achievement" >
        </activity>

        <!-- View Constitution Activity -->
        <activity
            android:name=".Constitution"
            android:label="View Constitution" >
        </activity>

        <!-- View Contribution Activity -->
        <activity
            android:name=".Contribution"
            android:label="View Contribution" >
        </activity>

        <!-- View Start Activity -->
        <activity
            android:name=".Start"
            android:label="Start" >
        </activity>
        <activity
            android:name=".Members"
            android:label="Start" >
        </activity>
    </application>

    <!--  Network Permissions -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--  Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
